  "private": true,
  "scripts": {
    "seed:config": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config -c prod",
    "seed:run": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed -c prod",
    "schema:drop": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop -c prod",
    "schema:sync": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync -c prod",
    "ui:dev": "npx lerna run --scope frontend start",
    "ui:build": "npx lerna run --scope frontend build",
    "ui:i18n": "npx lerna run --scope frontend i18next-ext",
    "server:dev": "npx ts-node -r tsconfig-paths/register src/server.ts",
    "lint": "npx eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "dependencies": {
    "@koa/router": "^10.0.0",
    "@ridi/epub-parser": "^0.7.2-alpha.4",
    "add": "^2.0.6",
    "apollo-server": "^2.22.2",
    "apollo-server-koa": "^2.22.2",
    "axios": "^0.21.1",
    "base-64": "^1.0.0",
    "class-validator": "^0.13.1",
    "faker": "^5.5.2",
    "googleapis": "^70.0.0",
    "graphql": "^15.5.0",
    "graphql-relay": "^0.6.0",
    "graphql-upload": "^11.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-static": "^5.0.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "tsconfig-paths": "^3.9.0",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.31",
    "typeorm-seeding": "^1.6.1",
    "typeorm-typedi-extensions": "^0.4.1",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/runtime": "^7.13.10",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "lerna": "^4.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
