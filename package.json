{
  "name": "kutub-teqween",
  "version": "1.0.0",
  "description": "Kutub by Teqween.com",
  "main": "index.js",
  "repository": "git@github.com:daddou-ma/kutub.git",
  "author": "Mohamed El Amine DADDOU <mea.daddou@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "seed:config": "npx lerna run --scope @kutub/backend seed:config --stream",
    "seed:run": "npx lerna run --scope @kutub/backend seed:run --stream",
    "schema:drop": "npx lerna run --scope @kutub/backend schema:drop --stream",
    "schema:sync": "npx lerna run --scope @kutub/backend schema:sync --stream",
    "ui:dev": "npx lerna run --scope @kutub/frontend start --stream",
    "ui:build": "npx lerna run --scope @kutub/frontend build --stream",
    "ui:i18n": "npx lerna run --scope @kutub/frontend i18next-ext --stream",
    "server:dev": "npx lerna run --scope @kutub/backend server:dev --stream",
    "server:start": "npx lerna run --scope @kutub/backend server:start --stream",
    "server:stop": "npx lerna run --scope @kutub/backend server:stop --stream",
    "lint": "npx eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/runtime": "^7.13.10",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "lerna": "^4.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
