{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2018",
    "module": "commonjs",
    "lib": ["dom", "es2018", "esnext.asynciterable"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "rootDir": "./",
    "baseUrl": "./",
    "skipLibCheck": true,
    "paths": {
      "Database/*": [
        "./src/database/*"
      ],
      "Enums/*": [
        "./src/enums/*"
      ],
      "Interfaces/*": [
        "./src/interfaces/*"
      ],
      "Modules/*": [
        "src/modules/*"
      ],
    },
  }
}
